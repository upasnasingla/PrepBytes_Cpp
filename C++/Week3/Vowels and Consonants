#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
    //write your code here
    int t;
    cin>>t;
    while(t!=0){
      string s;
      cin>>s;
      int countv=0;
      int countc=0;
      for(int i=0;i<s.length();i++){
        if(s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O'||s[i]=='U'){
          countv=countv+1;
        }
        else{
          countc=countc+1;
        }
        
      }
      cout<<countv<<" "<<countc;
      
      cout<<endl;
      t=t-1;
      
    }
    
    return 0;
  }
