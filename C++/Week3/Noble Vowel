#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
    //write your code here
    int t;
    cin>>t;
    while(t!=0){
      string s;
      cin>>s;
        bool flag=true;
        for(int j=0;j<s.length();j++){
           
          if(s[j]!='a'&&s[j]!='e'&&s[j]!='i'&&s[j]!='o'&&s[j]!='u'&&s[j]!='n'){
            if((s[j+1])!='a'&&(s[j+1])!='e'&&(s[j+1])!='i'&&(s[j+1])!='o'&&(s[j+1])!='u'){
              flag=false;
              
            }
          }
          
        }
        
        if(flag==true){
          cout<<"YES";
        }
        else{
          cout<<"NO";
        }
      cout<<endl;  
      t=t-1;
    }
    
    return 0;
  }
