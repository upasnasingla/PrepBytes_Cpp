#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
     char ch;
     cin>>ch;
     for(int i=1;i<=5;i++){
       for(int j=1;j<=i;j++){
         cout<<j;
       }
       for(int spaces=1;spaces<=(10-(2*i));spaces++){
         cout<<" ";
       }
       for(int j=i;j>=1;j--){
         cout<<j;
      
       }
       cout<<endl;
     }
    
    return 0;
  }
