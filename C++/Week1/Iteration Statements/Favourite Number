#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
    //write your code here
    int t;
    cin>>t;
    while(t!=0){
      int n;
      cin>>n;
      int count=0;
      while(n>0){
        int rem=n%10;
        if(rem==5){
         count+=1;
      }
      n=n/10;
      }
      cout<<count<<endl;
      

      t--;
    }
    
    return 0;
  }
