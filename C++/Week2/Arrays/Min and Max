#include <bits/stdc++.h>
using namespace std;
  
  int main()
  {
    //write your code here
    int t;
    cin>>t;
    while(t!=0){
      int n;
     cin>>n;
     int i;
     int arr[n];
     for(i=0;i<n;i++){
       cin>>arr[i];
     }
     int min=arr[0];
     
     for(int i=1;i<n;i++){
       if(arr[i]<min){
         min=arr[i];
       }
     }
     cout<<min<<" ";
     int max=arr[0];
     for(int i=0;i<n;i++){
       if(arr[i]>max){
         max=arr[i];
       }
     }
     cout<<max<<endl;
     
     t=t-1; 
    }
    
    
    
    return 0;
  }
