#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
    int t;
    cin>>t;
    while(t!=0){
      int n;
      cin>>n;
      int sum=1;
      int arr[n];
      for(int i=0;i<n;i++){
        cin>>arr[i];
      }
    
      for(int i=0;i<n;i++){
        sum=sum*arr[i];
      }
      cout<<sum<<endl;
      t=t-1;
    }
    
    
   
    return 0;
   
  }
